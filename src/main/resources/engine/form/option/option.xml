<?xml version="1.0" encoding="UTF-8"?>
<tsoftware  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
			xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
<model_layer>
<!-- 选项类型model -->
	<object name="option" table="t_choose" primary="id" unique="name" order="">
		<property name="id" type="Long" length="20" initial="" field="id"/>
		<property name="name" type="String" length="50" initial="" field="name"/>
		<property name="createBy" type="Long" length="20" field="create_by"/>
		<property name="createTime" type="Date" length="" field="create_time"/>
		<property name="updateBy" type="Long" length="20" field="update_by"/>
		<property name="updateTime" type="Date" length="" field="update_time"/>
		<property name="det" type="String" length="20" field=""/>
 	</object>  
 	<!-- 选项值model -->
 	<object name="type" table="t_choose_value" primary="id" unique="name" order="">
		<property name="id" type="Long" length="20" initial="" field="id"/>
		<property name="name" type="String" length="50" initial="" field="name"/>
		<property name="chooseId" type="Long" length="20" field="choose_id"/>
		<property name="score" type="Long" length="20" field="score"/>
		<property name="resource" type="Long" length="20" field="resource"/>
		<property name="createBy" type="Long" length="20" field="create_by"/>
		<property name="createTime" type="Date" length="" field="create_time"/>
		<property name="updateBy" type="Long" length="20" field="update_by"/>
		<property name="updateTime" type="Date" length="" field="update_time"/>
		<property name="chooseName" type="String" length="50" field=""/>
		<property name="det" type="String" length="20" field=""/>
 	</object>  
 	
 	
</model_layer>
</tsoftware>
